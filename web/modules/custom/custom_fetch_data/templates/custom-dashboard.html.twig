{# custom-dashboard.html.twig #}

<div class="user-dashboard">
  <h2>User Data by Joining Year and Passing Year</h2>
  <table>
    <thead>
      <tr>
        <th>User ID</th>
        <th>Display Name</th>
        <th>Joining Year</th>
        <th>Passing Year</th>
      </tr>
    </thead>
    <tbody>
      {% for joining_data_row in joining_date_data %}
        <tr>
          <td>{{ joining_data_row.user_id }}</td>
          <td>
            {% for display_name_row in display_name_data %}
              {% if display_name_row.user_id == joining_data_row.user_id %}
                {{ display_name_row.display_name }}
              {% endif %}
            {% endfor %}
          </td>
          <td>{{ joining_data_row.joining_date }}</td>
          <td>
            {% for passing_year_row in passing_year_data %}
              {% if passing_year_row.user_id == joining_data_row.user_id %}
                {{ passing_year_row.passing_year }}
              {% endif %}
            {% endfor %}
          </td>
        </tr>
      {% endfor %}
    </tbody>
  </table>
</div>
